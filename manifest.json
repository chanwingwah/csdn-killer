{
    "name": "CSDN killer",
    "version": "0.0.1",
    "description": "从百度的搜索结果中排除CSDN的内容！",
    "permissions": [
      "https://www.baidu.com/*",
      "https://www.google.com/*",
      "https://www.google.com.hk/*",
      "webRequest",
      "webRequestBlocking"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
    },
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png"
    },
    "manifest_version": 2
  }